{
  "name": "react-ssr-app",
  "version": "1.0.0",
  "description": "React SSR Application",
  "main": "server/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:client": "webpack --mode=production  --config webpack/client/webpack.prod.js --color --progress",
    "build:server": "webpack --mode=production --config webpack/server/webpack.prod.js",
    "start": "cross-env NODE_ENV=development nodemon --exec webpack --mode=development --config webpack/server/webpack.dev.js",
    "start:production": "cross-env NODE_ENV=production node index.js",
    "production": "npm run clean && npm run build:client && npm run build:server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohamedsamara/react-ssr-app.git"
  },
  "keywords": [],
  "author": "Mohamed Samara (https://github.com/mohamedsamara)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mohamedsamara/react-ssr-app/issues"
  },
  "homepage": "https://github.com/mohamedsamara/react-ssr-app#readme"
}
